@model IEnumerable<TestProject.Models.Classes>
@{
    ViewBag.Title = "Электронный дневник";
}
<h3>Классы</h3>
<table class="table">
    <tr>
        <td>Название класса</td>
    </tr>
    @foreach (var stclass in Model)
    {
        <tr>
            <td><a asp-action="Students" asp-controller="Home" asp-route-classname="@stclass.ClassName">@stclass.ClassName</a></td>
            <td>
            <a asp-action="Delete" asp-controller="Home" asp-route-classname="@stclass.ClassName">Удалить</a>
            </td>
        </tr>
    }
</table>
<a asp-action="Add">Добавить</a>


