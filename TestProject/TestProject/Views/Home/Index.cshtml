@model IEnumerable<TestProject.Models.Classes>
@{
    ViewBag.Title = "Электронный дневник";
}
<h3>Классы</h3>
<table class="table">
    <tr>
        <td>Название класса</td>
    </tr>
    @foreach (var stclass in Model)
    {
        <tr>
            <td>@stclass.ClassName</td>
        </tr>
    }
</table>
<a asp-action="Add">Добавить</a>
